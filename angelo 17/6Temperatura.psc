Algoritmo temperatura
	//definir codigo
	Definir gradosCelsius, gradosFahrenheit Como Real
	//tomar datos
	Escribir "digite la temperatura en grados celsius:", gradosCelsius;
	Leer gradosCelsius
	//mostrar resultado
	gradosFahrenheit <- (gradosCelsius * 9 / 5) + 32 
	Escribir "la temperatura en grados fahrenhait es de:" gradosFahrenheit;
	Leer gradosFahrenheit
	
FinAlgoritmo
